# Database connection string - replace with your actual database URL
DATABASE_URL=postgresql://username:password@localhost:5432/cse340

# Session secret - replace with a random string
SESSION_SECRET=your-session-secret-here

# JWT/Access token secret - replace with a random string  
ACCESS_TOKEN_SECRET=your-jwt-secret-here

# Development port (optional, defaults to 5500)
PORT=5500

# Database SSL (set to true if using cloud database like Heroku)
DB_SSL=false

# Node environment
NODE_ENV=development